GF_AUTH_GENERIC_OAUTH_TOKEN_URL=http://example.domain/realms/${KC_REALM_NAME}/protocol/openid-connect/token
GF_AUTH_GENERIC_OAUTH_EMAIL_ATTRIBUTE_NAME=email
