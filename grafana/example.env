KC_REALM_NAME=grafana

GF_LOG_LEVEL=info
#GF_SERVER_HTTP_PORT=80
GF_ADMIN_USERNAME=ras
GF_ADMIN_PASSWORD=dva
GF_SERVER_ROOT_URL=http://example.domain/
GF_SERVER_COOKIE_SECURE=false
GF_SERVER_COOKIE_SAMESITE=disabled

GF_AUTH_GENERIC_OAUTH_ENABLED=true
