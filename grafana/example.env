GF_AUTH_GENERIC_OAUTH_ROLE_ATTRIBUTE_PATH=contains(roles[*], 'grafana_admin') && 'Admin' || 'Viewer'
GF_AUTH_GENERIC_OAUTH_GROUPS_ATTRIBUTE_PATH=groups
